	<!-- Don't fiddle with these unless you know how it works -->
	<!-- This is how I've set up the option to have various gear sets via input commands such as /th or /eva. They all work, however they may not be coded 'optimally' so if you can clean it up, feel free -->
	<variables>
		<var name="init">1</var>
		<var name="Set">High</var>
		<var name="Acc">0</var>
		<var name="EvasionSetActive">0</var>
		<var name="MDTSetActive">0</var>
		<var name="PDTSetActive">0</var>
		<var name="TH">0</var>
		<var name="TP">1750</var>
		<var name="Kite">1</var>
		<var name="Auto">0</var>
		<var name="WS">0</var>
		<var name="Stance">0</var>
		<var name="Proc">0</var>
		<var name="Samba">0</var>
	</variables>

	<inputcommands>
		<cmd input="/init">
			<setvar name="init" value="0" />
		</cmd>
		<cmd input="/killall">
			<setvar name="Nuke" value="DMG" />
			<setvar name="Macc" value="Low" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="TH" value="0" />
			<setvar name="Kite" value="0" />
			<addtochat color="4">KILLED IT ALL, son!</addtochat>
		</cmd>
		<cmd input="/report">
			<addtochat color="4">[TPset:] $Set [Nukes:] $Nuke [Magic Accuracy:] $Macc</addtochat>
		</cmd>
		<cmd input="/proc">
			<if advanced="$Proc=0">
				<setvar name="Proc" value="1" />
				<command>/macro set 2</command>
				<command>/exec proc1.txt</command>
				<addtochat color="4">PROC Mode Engaged</addtochat>
			</if>
			<elseif advanced="$Proc=1">
				<setvar name="Proc" value="0" />
				<command>/macro set 3</command>
				<command>/exec proc2.txt</command>
				<addtochat color="4">PROC Mode Disengaged</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/auto">
			<if advanced="$Auto=0">
				<setvar name="Auto" value="1" />
				<addtochat color="4">Auto Mode Engaged</addtochat>
			</if>
			<elseif advanced="$Auto=1">
				<setvar name="Auto" value="0" />
				<addtochat color="4">Auto Mode Disengaged</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/tp">
			<if advanced="$TP!=1000">
				<setvar name="TP" value="1000" />
				<addtochat color="4">AUTO TP SET TO 1000</addtochat>
			</if>
			<elseif advanced="$TP=1000">
				<setvar name="TP" value="1750" />
				<addtochat color="4">AUTO TP SET TO 1750</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/wskill">
			<if advanced="$WS=0">
				<setvar name="WS" value="1" />
				<setvar name="WSname" value="Blade: Metsu" />
				<addtochat color="4">Auto $WSname Enabled!</addtochat>
			</if>
			<elseif advanced="$WS=1">
				<setvar name="WS" value="2" />
				<setvar name="WSname" value="Blade: Hi" />
				<addtochat color="4">Auto $WSname Enabled!</addtochat>
			</elseif>
			<elseif advanced="$WS=2">
				<setvar name="WS" value="0" />
				<setvar name="WSname" value="None" />
				<addtochat color="4">Auto WS Disabled!</addtochat>
			</elseif>
		</cmd>
		<cmd input="/samba">
			<if advanced="$Samba=0">
				<setvar name="Samba" value="1" />
				<addtochat color="4">Haste Samba</addtochat>
			</if>
			<elseif advanced="$Samba=1">
				<setvar name="Samba" value="2" />
				<addtochat color="4">Drain Samba</addtochat>				
			</elseif>
			<elseif advanced="$Samba=2">
				<setvar name="Samba" value="0" />
				<addtochat color="4">NO SAMBA</addtochat>				
			</elseif>				
		</cmd>
		<cmd input="/stance">
			<if advanced="$Stance=0">
				<setvar name="Stance" value="1" />
				<addtochat color="4">Stance is set to YONIN</addtochat>
			</if>
			<elseif advanced="$Stance=1">
				<setvar name="Stance" value="2" />
				<addtochat color="4">Stance is set to INNIN</addtochat>				
			</elseif>
			<elseif advanced="$Stance=2">
				<setvar name="Stance" value="0" />
				<addtochat color="4">Stance is set to OFF</addtochat>				
			</elseif>			
		</cmd>
		<cmd input="/set">
			<if advanced="$Set=Low">
				<setvar name="Set" value="High" />
				<addtochat color="4">HIGHER HASTE TP SET ACTIVATED</addtochat>
			</if>
			<elseif advanced="$Set=High">
				<setvar name="Set" value="Low" />
				<addtochat color="4">LOWER HASTE TP SET ACTIVATED</addtochat>				
			</elseif>
			<else>
				<setvar name="Set" value="Low" />
				<addtochat color="4">SOMETHING WENT WRONG, LOWER HASTE TP SET ACTIVATED</addtochat>
			</else>
		</cmd>
		<cmd input="/acc">
			<if advanced="$Acc=1">
				<setvar name="Acc" value="0" />
				<addtochat color="4">ACCURACY SET OFF</addtochat>
			</if>
			<else>
				<setvar name="Acc" value="1" />
				<addtochat color="4">ACCURACY SET ACTIVATED</addtochat>
			</else>
		</cmd>
		<cmd input="/kite">
			<if advanced="$Kite=0">
				<setvar name="Kite" value="1" />
				<addtochat color="4">Kiting mode, son!</addtochat>
			</if>
			<else>
				<setvar name="Kite" value="0" />
				<addtochat color="4">Kiting mode OFF, son!</addtochat>
			</else>
		</cmd>
		
		<!-- Treasure Hunter Toggle (If you want to replace the /th into something more user friendly, you can alter it just below -->
		<cmd input="/th">
			<if advanced="$TH=0">
				<setvar name="TH" value="1" />
				<addtochat color="4">Treasure Hunter TP set enabled!</addtochat>
			</if>
			<else>
				<setvar name="TH" value="0" />
				<addtochat color="4">Treasure Hunter set TP disabled!</addtochat>
			</else>
		</cmd>

		<!-- Evasion set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/eva">
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<if advanced="$EvasionSetActive=0">
				<setvar name="EvasionSetActive" value="1" />
				<addtochat color="4">Evasion set enabled!</addtochat>
			</if>
			<else>
				<setvar name="EvasionSetActive" value="0" />
				<addtochat color="4">Evasion set disabled.</addtochat>
			</else>
		</cmd>
		<!-- MDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/mdt">
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<if advanced="$MDTSetActive=0">
				<setvar name="MDTSetActive" value="1" />
				<addtochat color="4">MDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="MDTSetActive" value="0" />
				<addtochat color="4">MDT set disabled.</addtochat>
			</else>
		</cmd>
		<!-- PDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/pdt">
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<if advanced="$PDTSetActive=0">
				<setvar name="PDTSetActive" value="1" />
				<addtochat color="4">PDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="PDTSetActive" value="0" />
				<addtochat color="4">PDT set disabled.</addtochat>
			</else>
		</cmd>
		
		<!-- Use /Dsamba to auto choose highest drain samba as /DNC -->
		<cmd input="/Dsamba">
			<if p_joblevel=">=70">
				<command>/ja "Drain Samba II"</command>
			</if>
			<else>
				<command>/ja "Drain Samba"</command>
			</else>
		</cmd>
		<cmd input="/test">
			<addtochat color="4">TESTING SOMETHING OUT RIGHT NOW</addtochat>
		</cmd>

	</inputcommands>
	
	
	<init>
        <command>/macro book 4</command>
		<if p_subjob="DNC"><command delay="500">/macro set 1</command></if>
		<if p_subjob="WAR"><command delay="500">/macro set 3</command></if>
		<else><command delay="500">/macro set 3</command></else>
   	</init>

		<!-- These are the rules to make all your gearsets work.
			Again, the code could be optimised, but do not touch them if you do not know what you're doing. This can break the entire XML -->
	<idlegear>
		<if advanced="$init=0">
			<setvar name="init" value="1" />
			<command>/exec nin.txt</command>
		</if> 
		<if eq_rring="Trizek Ring">
			<equip lock="true">
				<rring>Trizek Ring</rring>                
			</equip>
		</if>
		<if eq_rring="Endorsement Ring">
			<equip lock="true">
				<rring>Endorsement Ring</rring>                
			</equip>
