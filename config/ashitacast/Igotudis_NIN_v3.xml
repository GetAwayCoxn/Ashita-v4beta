			<ammo>Staunch Tathlum</ammo>
			<body>Hiza. Haramaki +2</body>
			<hands>Malignance Gloves</hands>
			<legs>Mummu Kecks +2</legs>
			<feet augment="S54988744212505">Herculean Boots</feet>
			<neck>Loricate torque +1</neck>
			<waist>Resolute Belt</waist>
			<ear1>Odnowa Earring</ear1>
			<ear2>Odnowa Earring +1</ear2>
			<ring1>Defending Ring</ring1>
			<ring2>Purity Ring</ring2>
			<back augment="S2554632705541542543">Andartia's Mantle</back>
		</set>

		<!-- Same as above, you can type /pdt to turn on this set, if you have one -->
		<set name="PDT"  baseset="Evasion">
			<ammo>Staunch Tathlum</ammo>
			<head>Mpaca's Cap</head>
			<hands>Malignance Gloves</hands>
			<legs>Mummu Kecks +2</legs>
			<feet augment="S221218606901510265">Herculean Boots</feet> <!-- TA +3 -->
			<neck>Loricate torque +1</neck>
			<waist>Flume Belt +1</waist>
			<ear1>Odnowa Earring</ear1>
			<ear2>Odnowa Earring +1</ear2>
			<ring1>Defending Ring</ring1>
			<ring2>Gelatinous Ring +1</ring2>
			<back augment="S2554632705541542543">Andartia's Mantle</back>
		</set>

		<!-- TP SETS BUILT AROUND HASTE, LOW FOR NORMAL ISH, HIGH FOR 30%+ -->
		<set name="TP-Low"> <!-- AIM FOR 25% DUAL WEILD, 39% to cap with zero haste -->
			<ammo>Date Shuriken</ammo>
			<head>Adhemar Bonnet +1</head>
			<neck>Ninja Nodowa +1</neck> 
			<ear1>Eabani Earring</ear1> 
			<ear2>Cessance Earring</ear2>
			<body>Hiza. Haramaki +2</body>
			<hands>Adhemar Wrist. +1</hands>
			<lring>Petrov Ring</lring>
			<rring>Epona's Ring</rring>
			<legs>Tatena. Haidate +1</legs>
			<feet augment="S221218606901510265">Herculean Boots</feet>
			<back augment="S2554632705541542543">Andartia's Mantle</back>
			<waist>Sailfi Belt +1</waist>
		</set>

		<set name="TP-High"> <!-- AIM FOR 12% DUAL WEILD, 1% IF CAPPED HASTE -->
			<ammo>Date Shuriken</ammo> <!-- Date shur. == Happo +1  -->
			<head>Adhemar Bonnet +1</head> <!-- SU3 +1 better for tanking == adhemar +1 >   -->
			<body>Herculean Vest</body> <!-- SU3 +1 >  SU3 nq   -->
			<hands>Adhemar Wrist. +1</hands>  <!-- SU3 +1 for tank >  adhemar +1 for TP   -->
			<legs>Tatena. Haidate +1</legs> <!-- SU3 +1 for tank > samnuha tights str10/dex10/dblattk3/triple3 for TP  -->
			<feet augment="S221218606901510265">Herculean Boots</feet> <!-- TA +3 --> <!-- SU3 +1 for tank > herc boots must have TA4 w/dex/acc/attk for TP   -->
			<neck>Ninja Nodowa +1</neck>  <!-- JSE +2 > JSE +1 > JSE nq > portus collar > Moonbeam Nodowa -->
			<waist>Sailfi Belt +1</waist> <!-- windbuffet +1 is barely better and for all jobs -->
			<ear1>Brutal Earring</ear1> <!-- DONE -->
			<ear2>Cessance Earring</ear2> <!-- DONE -->
			<ring1>Defending Ring</ring1> <!-- Gere ring > Hetairoi ring   -->
			<ring2>Epona's Ring</ring2> <!-- done -->
			<back augment="S2554632705541542543">Andartia's Mantle</back> <!-- finish upgrading with more acc and dmg- -->
		</set>

		<set name="TP-Acc" baseset="TP-Low">
			<ammo>Date Shuriken</ammo>
			<head>Mummu Bonnet +2</head> <!-- replace with SU3 +1 eventually -->
			<body>Mummu Jacket +2</body>
			<hands>Malignance Gloves</hands>
			<legs>Tatena. Haidate +1</legs>
			<feet>Mummu Gamash. +2</feet>
			<neck>Ninja Nodowa +1</neck> 
			<waist>Sailfi Belt +1</waist>
			<ear1>Odr Earring</ear1>
			<ear2>Cessance Earring</ear2>
			<ring1>Mummu Ring</ring1>
			<ring2>Cacoethic Ring</ring2>
		</set>

     	<!-- This will make sure you use all your haste gear on Ichi: Ni and others -->
