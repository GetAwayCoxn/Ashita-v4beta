			<back augment="S255541914018572">Andartia's Mantle</back> <!-- add dmg down, make second cape with mattk instead of FC -->
		</set>
		
		<set name="Macc"> <!--need earrings -->
			<ammo>Staunch Tathlum</ammo> <!-- and yamarang in ammo slot -->
			<head>Mummu Bonnet +2</head> <!--Replace ASAP, relic reforged gear probably, or +2 Mummu -->
			<body>Mummu Jacket +2</body><!-- replace with malig gear -->
			<hands>Malignance Gloves</hands>
			<legs>Mummu Kecks +2</legs><!-- replace with malig gear -->
			<feet>Mummu Gamash. +2</feet> 
			<neck>Sanctity Necklace</neck>
			<waist>Salire Belt</waist>
			<ear2>Gwati Earring</ear2>
			<ring1>Mummu Ring</ring1><!-- Stikini ring's -->
			<ring2>Kishar Ring</ring2> <!-- Stikini ring's -->
			<back augment="S255541914018572">Andartia's Mantle</back> <!-- add dmg down, make second cape with mattk instead of FC -->
		</set>
		
		<set name="Ranged">
			<neck>Marked Gorget</neck>
			<ammo>Koga Shuriken</ammo>
			<hands>Ninja Tekko</hands>
			<legs>Ninja Hakama</legs>
			<ring2>Dingir Ring</ring2>
		</set>
		
		<set name="BladeHi">
			<!--  Kannagi > offhand = fudo masamune > ambuscade >    -->
			<ammo>Yetshila</ammo> <!-- need to get the +1 of this -->
			<head>Mummu Bonnet +2</head> <!-- AF +3 > SU3 +1 > Imp. wing hairpin +1 aug'd > Mummu +2 > SU3 nq > AF +2 -->
			<body>Mummu Jacket +2</body> <!-- SU3 HQ > Mummu +2 > SU3    -->
			<hands>Mummu Wrists +2</hands> <!-- Done  -->
			<legs>Mummu Kecks +2</legs> <!-- Relic +3 > Mummu +2 > SU3 +1   -->
			<feet>Mummu Gamash. +2</feet> <!-- Done -->
			<neck>Ninja Nodowa +1</neck>  <!-- JSE +2 > JSE +1 > JSE nq > rancor collar w/max hate > Fotia gorget > nefarious collar +1   -->
			<waist>Sailfi Belt +1</waist> <!-- Done  -->
			<ear1>Brutal Earring</ear1> <!-- Done  -->
			<ear2>Odr Earring</ear2> <!-- Done  -->
