			<hands>Hattori Tekko +1</hands> <!-- DONE -->
			<legs>Mummu Kecks +2</legs> <!-- Gyve trousers from Dawn II HTMB -->
			<feet>Hachi. Kyahan +1</feet> <!--Upgrade to +2, else relic feet +3 -->
			<neck>Sanctity Necklace</neck>
			<waist>Salire Belt</waist> <!-- escha stone -->
			<ear1>Hecate's Earring</ear1>
