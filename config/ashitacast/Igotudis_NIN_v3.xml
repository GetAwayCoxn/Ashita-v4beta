		<set name="Haste">
			<head>Adhemar Bonnet +1</head>
			<body>Rawhide Vest</body>
			<hands>Malignance Gloves</hands>
			<legs>Hiza. Hizayoroi +2</legs>
			<feet>Mpaca's Boots</feet>
			<neck>Moonbeam Necklace</neck>
			<waist>Resolute Belt</waist>
			<ear1>Loquac. Earring</ear1>
			<ear2>Odnowa Earring +1</ear2>
			<ring1>Prolix Ring</ring1>
			<ring2>Gelatinous Ring +1</ring2>
			<back augment="S2554632705541542543">Andartia's Mantle</back>
		</set>
		
		<set name="Utsusemi" baseset="Haste">
			<ammo>Staunch Tathlum</ammo>	
			<neck>Magoraga Beads</neck>
