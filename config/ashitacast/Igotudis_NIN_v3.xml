<?xml version="1.0" encoding="UTF-8"?>


<ashitacast>
	<settings>
		<buffupdate>true</buffupdate>
		<statusupdate>true</statusupdate>
		<hpupdate>false</hpupdate>
		<autoupdate>true</autoupdate>
	</settings>

	<sets>
		<!-- This is your idle set. The set you're in when not engaged-->
		<!-- It's best to put in your +movespeed gear and defensives-->
		<!-- Or whatever makes you look most like the dashing rogue you are-->
		<set name="Resting">
		
		</set>
		<set name="Idle">
			<ammo>Date Shuriken</ammo>
			<head>Mpaca's Cap</head>
			<ear1>Eabani Earring</ear1>
			<ear2>Etiolation Earring</ear2>
			<body>Hiza. Haramaki +2</body>
			<hands>Malignance Gloves</hands>
			<legs>Mummu Kecks +2</legs>
			<feet>Hachi. Kyahan +1</feet>
			<neck>Sanctity Necklace</neck>
			<waist>Flume Belt +1</waist>
			<back augment="S2554632705541542543">Andartia's Mantle</back>
			<ring1>Defending Ring</ring1>
			<ring2>Gelatinous Ring +1</ring2>
		</set>

		<!-- You can turn on your evasion set to be 'default TP set' by typing /eva -->
		<set name="Evasion" baseset="TP-High"> <!-- replace with purple set -->
			<head>Mpaca's Cap</head>
			<body>Hiza. Haramaki +2</body>
			<hands>Malignance Gloves</hands>
			<legs>Hiza. Hizayoroi +2</legs>
			<feet>Mpaca's Boots</feet>
			<ring1>Defending Ring</ring1>
			<ear1>Eabani Earring</ear1>
		</set>
		
		<!-- You can turn an an MDT set here by typing /mdt -->
