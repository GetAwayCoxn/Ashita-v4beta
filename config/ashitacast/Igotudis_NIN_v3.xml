			<back augment="S255463270718759">Andartia's Mantle</back>
		</set>
		
		<set name="BladeShun" baseset="WS">
			<body>Mummu Jacket +2</body>
			<neck>Fotia Gorget</neck>
			<waist>Fotia Belt</waist>
			<ear1>Odr Earring</ear1>
			<ear2>Lugra Earring +1</ear2>
		</set>
		
		<set name="BladeTen" baseset="WS">
			<ear2>Moonshade Earring</ear2>
		</set>
		
		<set name="BladeMetsu" baseset="WS">
			<ammo>Voluspa Tathlum</ammo><!-- V tathlum 1000 dom points for ammo -->
