	<commands>
		<command input="/init">
			<setvar name="init" value="0" />
		</command>
		<command input="/reset">
			<setvar name="Set" value="Main" />
			<setvar name="MDT" value="0" />
			<setvar name="PDT" value="0" />
			<echo mode="4">ALL VARIABLES RESET TO DEFAULT</echo>
		</command>
		<command input="/auto">
			<if advanced="$u_Auto=0">
				<setvar name="Auto" value="1" />
				<echo mode="4">Auto Mode Engaged</echo>
			</if>
			<elseif advanced="$u_Auto=1">
				<setvar name="Auto" value="0" />
				<echo mode="4">Auto Mode Disengaged</echo>
			</elseif>
		</command>
		<command input="/tp">
			<if advanced="$u_TP!=1000">
				<setvar name="TP" value="1000" />
				<echo mode="4">AUTO TP SET TO 1000</echo>
			</if>
			<elseif advanced="$u_TP=1000">
				<setvar name="TP" value="1850" />
				<echo mode="4">AUTO TP SET TO 1850</echo>
			</elseif>
		</command>
		<command input="/acc">
			<if advanced="$u_Set=Main">
				<setvar name="Set" value="Acc" />
				<echo mode="4">ACC SET</echo>
			</if>
			<elseif advanced="$u_Set=Acc">
				<setvar name="Set" value="Hybrid" />
				<echo mode="4">HYBRID SET</echo>
			</elseif>
			<else>
				<setvar name="Set" value="Main" />
				<echo mode="4">MAIN SET</echo>
			</else>
		</command>
		<command input="/learn">
			<if advanced="$u_Learn=0">
				<command>/ac disable hands</command>
				<setvar name="Learn" value="1" />
				<echo mode="4">BLUE HANDS LOCKED</echo>
				<equip>
					<hands>Assim. Bazu. +1</hands>
				</equip>
			</if>
			<else>
				<command>/ac enable hands</command>
				<setvar name="Learn" value="0" />
				<echo mode="4">HANDS UNLOCKED!</echo>
			</else>
		</command>
		<command input="/mdt">
			<setvar name="PDT" value="0" />
			<if advanced="$u_MDT=0">
				<setvar name="MDT" value="1" />
				<echo mode="4">MDT SET ENABLED</echo>
			</if>
			<else>
				<setvar name="MDT" value="0" />
				<echo mode="4">MDT SET DISABLED</echo>
			</else>
		</command>
		<command input="/pdt">
			<setvar name="MDT" value="0" />
