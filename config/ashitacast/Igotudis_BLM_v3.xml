	<variables>
		<var name="Status">0</var>
		<var name="DT">Off</var>
		<var name="Ring">None</var>
		<var name="Set">Nuke</var>
		<var name="Weapon">Club</var>
		<var name="Element">0</var>
		<var name="Dot">Ionohelix</var>
		<var name="Weather">Thunderstorm</var>
		<var name="Nuke">Thunder</var>
		<var name="Cape">false</var>
		<var name="Body">Off</var>
	</variables>
	
	
	
	<inputcommands>
		<cmd input="/status">
			<setvar name="Status" value="0" />
		</cmd>
		<cmd input="/dt">
			<if advanced="$DT=Off">
                <setvar name="DT" value="On"/>
            </if>
			<else>
				<setvar name="DT" value="Off"/>
			</else>
			<setvar name="Status" value="0" />
		</cmd>
		<cmd input="/cape">
			<if advanced="$Cape=false">
				<setvar name="Cape" value="true" />
				<command>/exec capeon.txt</command>
				<addtochat color="4">CP CAPE ON</addtochat>
			</if>
			<else>
				<setvar name="Cape" value="false" />
				<command>/exec capeoff.txt</command>
				<addtochat color="4">CP CAPE OFF</addtochat>				
			</else>		
		</cmd>
		<cmd input="/af">
			<if advanced="$Body=Off">
				<setvar name="Body" value="On" />
			</if>
			<else>
				<setvar name="Body" value="Off" />			
			</else>
			<setvar name="Status" value="0" />
		</cmd>
		<cmd input="/staff">
			<if advanced="$Weapon=Club">
				<setvar name="Weapon" value="Staff" />
			</if>
			<else>
				<setvar name="Weapon" value="Club" />
			</else>
			<setvar name="Status" value="0" />
		</cmd>		
		<cmd input="/burst">
			<if advanced="$Set=Nuke">
				<setvar name="Set" value="Burst" />
			</if>
			<else>
				<setvar name="Set" value="Nuke" />
			</else>
			<setvar name="Status" value="0" />
		</cmd>
		<cmd input="/wcycle">
			<if advanced="$Element=0">
				<setvar name="Element" value="1" />
				<addtochat color="4">Weather/Nukes set to BLIZZARD</addtochat>
				<setvar name="Dot" value="Cryohelix" />
				<setvar name="Weather" value="Hailstorm" />
				<setvar name="Nuke" value="Blizzard" />
			</if>
			<elseif advanced="$Element=1">
				<setvar name="Element" value="2" />
				<addtochat color="4">Weather/Nukes set to FIRE</addtochat>
				<setvar name="Dot" value="Pyrohelix" />
				<setvar name="Weather" value="Firestorm" />
				<setvar name="Nuke" value="Fire" />
			</elseif>
			<elseif advanced="$Element=2">
				<setvar name="Element" value="3" />
				<addtochat color="4">Weather/Nukes set to AERO</addtochat>
				<setvar name="Dot" value="Anemohelix" />
				<setvar name="Weather" value="Windstorm" />
				<setvar name="Nuke" value="Aero" />
			</elseif>
			<elseif advanced="$Element=3">
				<setvar name="Element" value="4" />
				<addtochat color="4">Weather/Nukes set to STONE</addtochat>
				<setvar name="Dot" value="Geohelix" />
				<setvar name="Weather" value="Sandstorm" />
				<setvar name="Nuke" value="Stone" />
			</elseif>
			<elseif advanced="$Element=4">
				<setvar name="Element" value="5" />
				<addtochat color="4">Weather/Nukes set to WATER</addtochat>
				<setvar name="Dot" value="Hydrohelix" />
				<setvar name="Weather" value="Rainstorm" />
				<setvar name="Nuke" value="Water" />
			</elseif>
			<elseif advanced="$Element=5">
				<setvar name="Element" value="0" />
				<addtochat color="4">Weather/Nukes set to THUNDER</addtochat>
				<setvar name="Dot" value="Ionohelix" />
				<setvar name="Weather" value="Thunderstorm" />
				<setvar name="Nuke" value="Thunder" />
			</elseif>
		</cmd>
		<cmd input="/helix">
			<command>/ma "$Dot" </command>
		</cmd>
		<cmd input="/weather">
			<command>/ma "$Weather" me</command>
		</cmd>
		<cmd input="/nuke1">
			<command>/ma "$Nuke" </command>
		</cmd>
		<cmd input="/nuke2">
			<command>/ma "$Nuke II" </command>
		</cmd>
		<cmd input="/nuke3">
			<command>/ma "$Nuke III" </command>
		</cmd>
		<cmd input="/nuke4">
			<command>/ma "$Nuke IV" </command>
		</cmd>
		<cmd input="/nuke5">
			<command>/ma "$Nuke V" </command>
		</cmd>
		<cmd input="/nuke6">
			<command>/ma "$Nuke VI" </command>
		</cmd>
	</inputcommands>


 
	<idlegear>
		<if advanced="$Status=0">
			<setvar name="Status" value="1" />
			<addtochat color="4">%p_mainjob/%p_subjob STATUS:   DT [$DT]   SET [$Set]   WEAPON [$Weapon]   CYCLE [$Nuke]   AF BODY [$Body]</addtochat>
		</if>  



		<if advanced="$DT=On"> <!-- Force DT set if manually engaged with /dt command -->
			<equip set="DT" />
		</if>
		<elseif p_status="engaged">
			<equip set="TP" />
		</elseif>
		<else>
			<equip set="Idle-$Weapon" />
			<if p_ismoving="True">
				<equip set="Movement" />
			</if>
		</else>



		<!-- Town gear and Ring locks, I only use my right ring for these -->
		<if e_area="Ru'Lude*|Nashmau|Tavnazian Safehold|Port*|Bastok*|Windurst*|*San d'Oria|*Jeuno|*Whitegate|*Adoulin|Selbina|Mhaura|Norg|Rabao|Mog*">
			<equip set="Town" />
		</if>
		<if eq_rring="Trizek*|Capacity*|Endorsement*|Facility*|Dim. Ring*|Echad*|Warp*">
			<setvar name="Ring" value="%eq_rring" />
			<equip><rring>$Ring</rring></equip>
		</if>
		<if BuffActive="15"><!-- Doom -->
			<equip>
				<lring>Blenmot's Ring</lring>
				<rring>Purity Ring</rring>
			</equip>
		</if>	
		<if BuffActive="437"><!-- Mana Wall -->
			<equip>
				<feet>Goetia Sabots +2</feet>
				<back>Taranus's Cape</back>
			</equip>
		</if>
	</idlegear>


	
	<premagic>
		<equip set="Precast" />
		
		
		
		<if ad_type="whitemagic">      
			<if ad_skill="healingmagic">
				<if ad_name="Cure*|Cura*">
					<equip set="CurePre" />
				</if>
			</if>
			<elseif ad_skill="enhancingmagic">
				<if ad_name="Stoneskin">
					<equip set="SSPre" />
				</if>
				<else>
					<equip set="EnhPre" />
				</else>
			</elseif>
		</if>
		
		
		
		<elseif ad_type="blackmagic">      		
			<if ad_skill="elementalmagic">
				<equip set="ElePre" />
				<if ad_name="Impact"><equip set="Impact" lock="true" /></if>
			</if>
			<elseif ad_skill="darkmagic">
				<if ad_name="Death"><equip set="DeathPre" lock="true" /></if>
			</elseif>
			<elseif ad_skill="enhancingmagic">
				<equip set="EnhPre" />
			</elseif>   
		</elseif>
	</premagic>

	<midmagic>
		<equip set="Haste"/>
		
		
		
		<if ad_type="whitemagic">
