<ashitacast>

	<sets>
		<set name="Resting">
		</set>
		<set name="Idle">
			<ammo>Voluspa Tathlum</ammo>
			<neck>Empath Necklace</neck>
			<head>Emicho Coronet</head>
			<body>Tali'ah Manteel +1</body>
			<hands>Ankusa Gloves</hands>
			<legs>Tali'ah Sera. +2</legs>
			<feet>Tali'ah Crackows +1</feet>
			<waist>Hurch'lan Sash</waist>
			<ring1>Tali'ah Ring</ring1>
			<ring2>Meghanada Ring</ring2>
			<back augment="Pet: Acc.+20,Pet: R.Acc.+20,Pet: Atk.+20,Pet: R.Atk.+20,Pet: 'Regen'+10">Artio's Mantle</back>
		</set>
		<set name="Evasion">
			<ammo>Yamarang</ammo>
			<head>Pill. Bonnet +2</head>
			<body>Emet Harness +1</body>
			<hands>Meg. Gloves +2</hands>
			<legs>Pill. Culottes +2</legs>
			<feet>Meg. Jam. +1</feet>
			<neck>Loricate Torque +1</neck>
			<waist>Hurch'lan Sash</waist>
			<ear1>Odnowa Earring</ear1>
			<ear2>Odnowa Earring +1</ear2>
			<ring1>Moonbeam Ring</ring1>
			<ring2>Moonbeam Ring</ring2>
			<back>Moonbeam Cape</back>
		</set>
		<set name="MDT" baseset="PDT">
			<neck>Twilight Torque</neck>
		</set>
		<set name="PDT" baseset="Idle">
			<head>Meghanada Visor +1</head>
			<body>Tartarus Platemail</body>
			<hands>Meg. Gloves +2</hands>
			<legs>Meg. Chausses +2</legs>
			<feet>Meg. Jam. +1</feet>
			<neck>Diemer Gorget</neck>
			<back>Impassive Mantle</back>
			<ring1>Defending Ring</ring1>
		</set>
		<set name="TP-TP" baseset="Idle">
			<ammo>Voluspa Tathlum</ammo>
			<neck>Lissome Necklace</neck>
			<head>Tali'ah Turban +2</head>
			<body>Tali'ah Manteel +1</body>
			<hands>Tali'ah Gages +1</hands>
			<feet>Tali'ah Crackows +1</feet>
			<waist>Hurch'lan Sash</waist>
			<ear1>Steelflash Earring</ear1>
			<ear2>Bladeborn Earring</ear2>
			<ring1>Tali'ah Ring</ring1>
			<ring2>Meghanada Ring</ring2>
			<back augment="DEX+20,Accuracy+20,Attack+20,Pet: Accuracy+6,Pet: Rng. Acc.+6,'Store TP'+10">Artio's Mantle</back>
		</set>
		<set name="TP-PDT" baseset="TP-TP">
			<neck>Empath Necklace</neck>
			<legs>Tali'ah Sera. +2</legs>
			<back augment="Pet: Acc.+20,Pet: R.Acc.+20,Pet: Atk.+20,Pet: R.Atk.+20,Pet: 'Regen'+10">Artio's Mantle</back>
		</set>
		<set name="Haste">
			<head>Skulker's Bonnet +1</head>
			<neck>Magoraga Beads</neck>
			<waist>Hurch'lan Sash</waist>
			<ear1>Loquac. Earring</ear1>
			<ear2>Eabani Earring</ear2>
			<ring1>Prolix Ring</ring1>
			<ring2>Moonbeam Ring</ring2>
			<back>Canny Cape</back>
		</set>
		<set name="Ready" baseset="TP">
			<ammo>Voluspa Tathlum</ammo>
			<head>Emicho Coronet</head>
			<body>Valorous Mail</body>
			<legs>Despair Cuisses</legs>
			<neck>Empath Necklace</neck>
			<waist>Incarnation Sash</waist>
			<back augment="Pet: Acc.+20,Pet: R.Acc.+20,Pet: Atk.+20,Pet: R.Atk.+20,Pet: 'Regen'+10">Artio's Mantle</back>
		</set>
		<set name="WS">
			<head>Valorous Mask</head>
			<body>Meg. Cuirie +1</body>
			<hands>Meg. Gloves +2</hands>
			<legs>Meg. Chausses +2</legs>
			<feet>Meg. Jam. +1</feet>
			<neck>Lissome Necklace</neck>
			<ear1>Thrud Earring</ear1>
			<ring1>Rajas Ring</ring1>
			<ring2>Flame Ring</ring2>
			<back augment="DEX+20,Accuracy+20,Attack+20,Pet: Accuracy+6,Pet: Rng. Acc.+6,'Store TP'+10">Artio's Mantle</back>
		</set>
		<set name="Ruinator" baseset="WS">
		</set>
		<set name="Mistral" baseset="WS">
		</set>
		<set name="Reward">
			<ammo>Pet Food Theta</ammo>
			<body>Monster Jackcoat</body>
			<feet>Monster Gaiters</feet>
			<ear1>Ferine Earring</ear1>
		</set>
		<set name="Call Beast">
			<hands>Ankusa Gloves</hands>
		</set>
		<set name="Waltz">
			<ammo>Yamarang</ammo>
			<head>Mummu Bonnet +1</head>
			<body>Herculean Vest</body>
			<hands>Macabre Gaunt.</hands>
			<legs>Pill. Culottes +2</legs>
			<neck>Unmoving Collar +1</neck>
			<waist>Chaac Belt</waist>
			<ear1>Oneiros Earring</ear1>
			<ear2>Eabani Earring</ear2>
			<ring1>Asklepian Ring</ring1>
			<ring2>Moonbeam Ring</ring2>
			<back>Laic Mantle</back>
		</set>
		<set name="Movement">
		</set>
	</sets>

	<commands>
		<command input="/killall">
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="EvasionSetActive" value="0" />
			<echo mode="4">KILLED IT ALL, son!</echo>
		</command>
		<command input="/set">
			<if advanced="$u_Set=TP">
				<setvar name="Set" value="PDT" />
				<echo mode="4">PDT TP Set</echo>
			</if>
			<elseif advanced="$u_Set=PDT">
				<setvar name="Set" value="TP" />
				<echo mode="4">Normal TP Set</echo>
			</elseif>
		</command>
		<command input="/auto">
			<if advanced="$u_Auto=0">
				<setvar name="Auto" value="1" />
				<echo mode="4">Auto Mode Engaged</echo>
			</if>
			<elseif advanced="$u_Auto=1">
				<setvar name="Auto" value="0" />
				<echo mode="4">Auto Mode Disengaged</echo>
			</elseif>
		</command>
		<command input="/tp">
			<if advanced="$u_TP!=1000">
				<setvar name="TP" value="1000" />
				<echo mode="4">AUTO TP SET TO 1000</echo>
			</if>
			<elseif advanced="$u_TP=1000">
				<setvar name="TP" value="1650" />
				<echo mode="4">AUTO TP SET TO 1650</echo>
			</elseif>
		</command>
		<command input="/samba">
			<if advanced="$u_Samba=0">
				<setvar name="Samba" value="1" />
				<echo mode="4">Haste Samba</echo>
			</if>
			<elseif advanced="$u_Samba=1">
				<setvar name="Samba" value="2" />
				<echo mode="4">Drain Samba</echo>
			</elseif>
			<elseif advanced="$u_Samba=2">
				<setvar name="Samba" value="0" />
				<echo mode="4">NO SAMBA</echo>
			</elseif>
		</command>
		<command input="/step">
			<if advanced="$u_Step=0">
				<setvar name="Step" value="1" />
				<command>/ja "Quickstep"</command>
				<echo mode="4">QUICK STEP</echo>
			</if>
			<elseif advanced="$u_Step=1">
				<setvar name="Step" value="0" />
				<command>/ja "Box Step"</command>
				<echo mode="4">BOX STEP</echo>
			</elseif>
		</command>
		<command input="/wskill">
			<if advanced="$u_WS=0">
				<setvar name="WS" value="1" />
				<setvar name="TP" value="1750" />
				<setvar name="WSname" value="Decimation" />
				<echo mode="4">Auto $u_WSname Enabled!</echo>
			</if>
			<elseif advanced="$u_WS=1">
				<setvar name="WS" value="2" />
				<setvar name="TP" value="1750" />
				<setvar name="WSname" value="Ruinator" />
				<echo mode="4">Auto $u_WSname Enabled!</echo>
			</elseif>
			<elseif advanced="$u_WS=2">
				<setvar name="WS" value="0" />
				<setvar name="TP" value="1750" />
				<setvar name="WSname" value="None" />
				<echo mode="4">Auto WS Disabled!</echo>
			</elseif>
		</command>
		<command input="/Dsamba">
			<if p_joblevel="<70">
				<command>/ja "Drain Samba"</command>
			</if>
			<else>
				<command>/ja "Drain Samba II"</command>
			</else>
		</command>
		<command input="/eva">
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<if advanced="$u_EvasionSetActive=0">
				<setvar name="EvasionSetActive" value="1" />
				<echo mode="4">Evasion set enabled!</echo>
			</if>
			<else>
				<setvar name="EvasionSetActive" value="0" />
				<echo mode="4">Evasion set disabled.</echo>
			</else>
		</command>
		<command input="/mdt">
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<if advanced="$u_MDTSetActive=0">
				<setvar name="MDTSetActive" value="1" />
				<echo mode="4">MDT set enabled!</echo>
			</if>
			<else>
				<setvar name="MDTSetActive" value="0" />
				<echo mode="4">MDT set disabled.</echo>
			</else>
		</command>
		<command input="/pdt">
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="MDTSetActive" value="0" />
			<if advanced="$u_PDTSetActive=0">
				<setvar name="PDTSetActive" value="1" />
				<echo mode="4">PDT set enabled!</echo>
			</if>
			<else>
				<setvar name="PDTSetActive" value="0" />
				<echo mode="4">PDT set disabled.</echo>
			</else>
		</command>
	</commands>

	<load>
		<setvar name="Set" value="TP" />
		<setvar name="EvasionSetActive" value="0" />
		<setvar name="MDTSetActive" value="0" />
		<setvar name="PDTSetActive" value="0" />
		<setvar name="WS" value="0" />
		<setvar name="TP" value="1000" />
		<setvar name="WSname" value="Decimation" />
		<setvar name="Auto" value="0" />
		<setvar name="Samba" value="0" />
		<setvar name="Step" value="0" />
		<command>/macro book 3</command>
		<if p_subjob="DNC">
			<command delay="500">/macro set 1</command>
		</if>
		<else>
			<command delay="500">/macro set 1</command>
		</else>
	</load>

	<default>
		<if eq_rring="Trizek Ring">
			<equip lock="true">
				<ring2>Trizek Ring</ring2>
			</equip>
		</if>
		<if eq_rring="Echad Ring">
			<equip lock="true">
				<ring2>Echad Ring</ring2>
			</equip>
		</if>
		<if eq_rring="Capacity Ring">
			<equip lock="true">
				<ring2>Capacity Ring</ring2>
			</equip>
		</if>
		<if eq_rring="Caliber Ring">
			<equip lock="true">
				<ring2>Caliber Ring</ring2>
			</equip>
		</if>
		<if eq_rring="Facility Ring">
			<equip lock="true">
				<ring2>Facility Ring</ring2>
			</equip>
		</if>
		<if eq_rring="Endorsement Ring">
			<equip lock="true">
				<ring2>Endorsement Ring</ring2>
			</equip>
		</if>
		<if eq_rring="Dim. Ring (Dem)">
			<equip lock="true">
				<ring2>Dim. Ring (Dem)</ring2>
			</equip>
		</if>
		<if eq_lring="Dim. Ring (Dem)">
			<equip lock="true">
				<ring1>Dim. Ring (Dem)</ring1>
			</equip>
		</if>
		<if eq_back="Mecisto. Mantle">
			<equip lock="true">
				<back>Mecisto. Mantle</back>
			</equip>
		</if>
		<if eq_rring="Warp Ring">
			<equip lock="true">
				<ring2>Warp Ring</ring2>
			</equip>
		</if>
		<elseif eq_lring="Warp Ring">
			<equip lock="true">
				<ring1>Warp Ring</ring1>
			</equip>
		</elseif>
		<elseif p_status="resting">
			<equip set="Resting" />
		</elseif>
		<elseif advanced="$u_EvasionSetActive=1">
			<equip set="Evasion" />
			<if p_ismoving="True">
				<equip set="Movement" />
			</if>
		</elseif>
		<elseif advanced="$u_MDTSetActive=1">
			<equip set="MDT" />
			<if p_ismoving="True">
				<equip set="Movement" />
			</if>
		</elseif>
		<elseif advanced="$u_PDTSetActive=1">
			<equip set="PDT" />
			<if p_ismoving="True">
				<equip set="Movement" />
			</if>
		</elseif>
		<else>
			<if p_status="engaged">
				<equip set="TP-$u_Set" />
			</if>
			<else>
				<equip set="Idle" />
				<if p_ismoving="True">
					<equip set="Movement" />
				</if>
			</else>
		</else>
		<if advanced="$u_Auto=1">
			<if p_status="engaged">
				<if p_subjob="DNC">
					<if advanced="$u_Samba=2">
						<if b_368=">=1">
						</if>
						<else>
							<if p_tp=">250">
								<command>/Dsamba</command>
							</if>
						</else>
					</if>
					<elseif advanced="$u_Samba=1">
						<if b_370=">=1">
						</if>
						<else>
							<if p_tp=">450">
								<command>//Hastesamba</command>
							</if>
						</else>
					</elseif>
				</if>
				<if advanced="$u_WS!=0">
					<if p_tp=">$u_TP">
						<command>/ws "$u_WSname"</command>
					</if>
				</if>
			</if>
		</if>
	</default>

	<preranged>
		<equip set="Ranged" />
	</preranged>

	<precast>
		<if ad_name="Utsusemi*">
			<equip set="Evasion" />
			<if ad_name="*Ni">
				<equip set="Haste" />
			</if>
		</if>
		<if ad_name="Monomi*|Tonko*">
			<equip set="Haste" />
		</if>
		<else>
			<equip set="Haste" />
		</else>
	</precast>

	<midcast>
		<if ad_name="Monomi*">
			<command>/cancel 71</command>
		</if>
		<elseif ad_name="Utsusemi: Ichi">
			<if b_66=">=1">
				<command>/cancel 66</command>
			</if>
			<elseif b_444=">=1">
				<command>/cancel 444</command>
			</elseif>
			<else>
				<command>/cancel 445</command>
			</else>
		</elseif>
		<if b_71=">=1">
			<command>/cancel 71</command>
			<cancel />
			<command delay="2000">/ja "$ad_name" $at_id</command>
		</if>
	</midcast>

	<ability>
		<if ad_name="Curing*">
			<equip set="Waltz" />
		</if>
		<elseif ad_name="Reward">
			<equip set="Reward" />
		</elseif>
		<elseif ad_name="Call Beast|Bestial Loyalty">
			<equip set="Call Beast" />
			<setvar name="Pet" value="1" />
			<echo mode="4">PET Mode Engaged</echo>
		</elseif>
		<elseif ad_name="Leave">
			<setvar name="Pet" value="0" />
			<echo mode="4">PET Mode Disengaged</echo>
		</elseif>
		<if ad_name="Spectral Jig">
			<if b_71=">=1">
				<command>/cancel 71</command>
				<cancel />
				<command delay="2000">/ja "$ad_name" $at_id</command>
			</if>
		</if>
	</ability>

	<weaponskill>
		<if ad_name="Ruinator">
			<equip set="Ruinator" />
		</if>
		<elseif ad_name="Mistral Axe">
			<equip set="Mistral" />
		</elseif>
		<else>
			<equip set="WS" />
		</else>
	</weaponskill>

	<petskill>
		<equip set="Ready" />
	</petskill>

</ashitacast>