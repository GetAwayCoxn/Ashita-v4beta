	<inputcommands>
		<cmd input="/init">
			<setvar name="init" value="0" />
		</cmd>
		<cmd input="/killall">
			<setvar name="Set" value="Old" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="Reraise" value="0" />
			<addtochat color="4">KILLED IT ALL, son!</addtochat>
		</cmd>
		<cmd input="/auto">
			<if advanced="$Auto=0">
				<setvar name="Auto" value="1" />
				<addtochat color="4">Auto Mode Engaged</addtochat>
			</if>
			<elseif advanced="$Auto=1">
				<setvar name="Auto" value="0" />
				<addtochat color="4">Auto Mode Disengaged</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/craft"> 
			<if advanced="$Craft=0">
				<setvar name="Craft" value="1" />
				<command>/ac disable</command>
				<equip set="WWHQ" />
				<addtochat color="4">WOODWORKING SET EQUIPED</addtochat>
			</if>
			<else>
				<setvar name="Craft" value="0" />
				<command>/ac enable</command>
				<addtochat color="4">WOODWORKING SET UNEQUIPED</addtochat>
			</else>		
		</cmd>
		<cmd input="/tp">
			<if advanced="$TP!=1000">
				<setvar name="TP" value="1000" />
				<addtochat color="4">AUTO TP SET TO 1000</addtochat>
			</if>
			<elseif advanced="$TP=1000">
				<setvar name="TP" value="1850" />
				<addtochat color="4">AUTO TP SET TO 1850</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/acc">
			<if advanced="$Set=Old">
				<setvar name="Set" value="Acc" />
				<addtochat color="4">Accuracy Set</addtochat>
			</if>
			<else>
				<setvar name="Set" value="Old" />
				<addtochat color="4">Attack Set</addtochat>
			</else>
		</cmd>
		<cmd input="/learn">
			<if advanced="$Learn=0">
				<command>/ac disable</command>
				<setvar name="Learn" value="1" />
				<addtochat color="4">BLUE HANDS LOCKED</addtochat>
				<equip><hands>Magus Bazubands</hands><legs>Carmine Cuisses</legs></equip>
