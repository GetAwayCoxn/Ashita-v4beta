	<!-- This is the master reset switch for your toggles. Should rest all of them. -->
	<cmd input="/killall">
			<setvar name="SIR" value="0" />
			<setvar name="Charm" value="0" />
			<setvar name="Death" value="0" />
			<setvar name="MagicEva" value="0" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="Reraise" value="0" />
			<addtochat color="159">KILLED IT ALL, son!</addtochat>
	</cmd>
	
	<cmd input="/report">
		<addtochat color="4">[TPset:] $Set [Nukes:] $Nuke </addtochat>
	</cmd>
		
	<cmd input="/tank">
			<if advanced="$Tank=0">
				<setvar name="Tank" value="1" />
				<addtochat color="4">AUTO DEFENDER AND MAJESTY ON</addtochat>
			</if>
			<else>
				<setvar name="Tank" value="0" />
				<addtochat color="4">AUTO DEFENDER AND MAJESTY OFF</addtochat>
			</else>
	</cmd>	

	<cmd input="/cape">
		<if advanced="$Cape=false">
			<setvar name="Cape" value="true" />
			<command>/exec capeon.txt</command>
			<addtochat color="4">CP CAPE ON</addtochat>
		</if>
		<else>
			<setvar name="Cape" value="false" />
			<command>/exec capeoff.txt</command>
			<addtochat color="4">CP CAPE OFF</addtochat>				
		</else>		
	</cmd>

	<cmd input="/maj">
			<if advanced="$Maj=0">
				<setvar name="Maj" value="1" />
				<addtochat color="4">AUTO MAJESTY ON</addtochat>
			</if>
			<else>
				<setvar name="Maj" value="0" />
				<addtochat color="4">AUTO MAJESTY OFF</addtochat>
			</else>
	</cmd>	
	
	<cmd input="/auto">
		<if advanced="$Auto=0">
			<setvar name="Auto" value="1" />
			<addtochat color="4">Auto Mode Engaged</addtochat>
		</if>
		<elseif advanced="$Auto=1">
			<setvar name="Auto" value="0" />
			<addtochat color="4">Auto Mode Disengaged</addtochat>				
		</elseif>		
	</cmd>
	
	<cmd input="/wskill">
		<if advanced="$WS=0">
			<setvar name="WS" value="1" />
			<setvar name="WSname" value="Chant du Cygne" />
			<addtochat color="4">Auto $WSname Enabled!</addtochat>
		</if>
		<elseif advanced="$WS=1">
			<setvar name="WS" value="2" />
			<setvar name="WSname" value="Requiescat" />
			<addtochat color="4">Auto $WSname Enabled!</addtochat>
		</elseif>
		<elseif advanced="$WS=2">
			<setvar name="WS" value="3" />
			<setvar name="WSname" value="Savage Blade" />
			<addtochat color="4">Auto $WSname Enabled!</addtochat>
		</elseif>
		<elseif advanced="$WS=3">
			<setvar name="WS" value="0" />
			<setvar name="WSname" value="None" />
			<addtochat color="4">Auto WS Disabled!</addtochat>
		</elseif>
	</cmd>
	
	<cmd input="/tp">
		<if advanced="$TP!=1000">
			<setvar name="TP" value="1000" />
			<addtochat color="4">AUTO TP SET TO 1000</addtochat>
		</if>
		<elseif advanced="$TP=1000">
			<setvar name="TP" value="1750" />
			<addtochat color="4">AUTO TP SET TO 1750</addtochat>				
		</elseif>		
	</cmd>
	
	<!-- Set toggle. If you add additional levels, make sure you update WS sets as well! -->
	<cmd input="/Set">
			<if advanced="$Set=Old">
				<setvar name="Set" value="Hybrid" />
				<addtochat color="4">HYBRID set on</addtochat>
			</if>
			<else>
				<setvar name="Set" value="Old" />
				<addtochat color="4">HYBRID set off</addtochat>
			</else>
	</cmd>
	<cmd input="/sir">
			<if advanced="$SIR=0">
				<setvar name="SIR" value="1" />
				<addtochat color="4">Spell interrupt set On!</addtochat>
			</if>
			<else>
				<setvar name="SIR" value="0" />
				<addtochat color="4">Spell interrupt set Off!</addtochat>
			</else>
	</cmd>		
	<cmd input="/charm">
			<if advanced="$Charm=0">
				<setvar name="Charm" value="1" />
				<addtochat color="4">Charm resist set On!</addtochat>
			</if>
			<else>
				<setvar name="Charm" value="0" />
				<addtochat color="4">Charm resist set Off!</addtochat>
			</else>
	</cmd>
	<cmd input="/death">

			<if advanced="$Death=0">
				<setvar name="Death" value="1" />
				<addtochat color="4">Death resist set On!</addtochat>
			</if>
			<else>
				<setvar name="Death" value="0" />
				<addtochat color="4">Death resist set Off!</addtochat>
			</else>
	</cmd>		
	<cmd input="/magiceva">
			<if advanced="$MagicEva=0">
				<setvar name="MagicEva" value="1" />
				<addtochat color="4">Magic Evasion set On!</addtochat>
			</if>
			<else>
				<setvar name="MagicEva" value="0" />
				<addtochat color="4">Magic Evasion set Off!</addtochat>
			</else>
	</cmd>
	<cmd input="/reraise">

			<if advanced="$Reraise=0">
				<setvar name="Reraise" value="1" />
				<addtochat color="4">Zombie mode, son!</addtochat>
			</if>
			<else>
				<setvar name="Reraise" value="0" />
				<addtochat color="4">Zombie mode OFF, son!</addtochat>
			</else>
		</cmd>
	
	<cmd input="/mdt">
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<if advanced="$MDTSetActive=0">
				<setvar name="MDTSetActive" value="1" />
				<addtochat color="4">MDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="MDTSetActive" value="0" />
				<addtochat color="4">MDT set disabled.</addtochat>
			</else>
	</cmd>
	<cmd input="/pdt">
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<if advanced="$PDTSetActive=0">
				<setvar name="PDTSetActive" value="1" />
				<addtochat color="4">PDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="PDTSetActive" value="0" />
				<addtochat color="4">PDT set disabled.</addtochat>
			</else>
	</cmd>
	</inputcommands>
	
	<init>
        <command>/macro book 11</command>
		<if p_subjob="DNC"><command delay="500">/macro set 9</command></if>
		<if p_subjob="WAR"><command delay="500">/macro set 10</command></if>
   	</init>

	<!-- These are all of the various rules that decide when to equip gear. Be careful when making
	any changes to them! -->
	<idlegear>
		<!-- When the file loads, this executes txt.txt in Ashita's /scripts folder to set my macro
		book and set and initiate lockstyle. Since I always forget to turn it on. -->
	<if advanced="$init=0">
			<setvar name="init" value="1" />
			<command>/exec nin.txt</command>
	</if>  

	<!-- Various rings or equipments you don't want Ashitacast to swap out. You will have to manually
	unequip them for gearswapping to resume as normal. -->
		<if eq_rring="Trizek Ring">
			<equip lock="true">
				<rring>Trizek Ring</rring>                
			</equip>
		</if>
		<if eq_rring="Capacity Ring">
			<equip lock="true">
				<rring>Capacity Ring</rring>                
			</equip>
		</if>

		<if eq_rring="Facility Ring">
			<equip lock="true">
				<rring>Facility Ring</rring>                
			</equip>
		</if>
		<if eq_rring="Endorsement Ring">
			<equip lock="true">
				<rring>Endorsement Ring</rring>                
			</equip>
		</if>
		<if eq_rring="Dim. Ring (Dem)">
			<equip lock="true">
				<rring>Dim. Ring (Dem)</rring>                
			</equip>
		</if>
		<if eq_rring="Warp Ring">
			<equip lock="true">
				<rring>Warp Ring</rring>				
			</equip>
		</if>

		<elseif advanced="$EvasionSetActive=1">
			<equip set="Evasion" />
		</elseif>

		<elseif advanced="$MagicEva=1">
			<equip set="MagicEva" />
				<if p_ismoving="True">
					<equip set="Movement" />
				</if>
			<if advanced="$Reraise=1">
				<equip>
					<head>Twilight Helm</head>
					<body>Twilight Mail</body>
				</equip>
			</if>
			<if advanced="$Charm=1">
				<equip set="Charm" />
			</if>
			<if advanced="$Death=1">
				<equip set="Death" />
			</if>			
			<if BuffActive="2">
				<equip>
					<neck>Vim Torque</neck>
				</equip>
			</if>
			<if BuffActive="15">
				<equip>
					<waist>Gishdubar Sash</waist>
					<lring>Saida Ring</lring>
					<rring>Purity Ring</rring>
				</equip> 
			</if>	
			<if advanced="$CP=1">
				<equip lock="true">
					<back>Mecisto. Mantle</back>                
				</equip>
			</if>				
		</elseif>	

		<elseif advanced="$MDTSetActive=1">
			<equip set="MDT" />
				<if p_ismoving="True">
					<equip set="Movement" />
				</if>
			<if advanced="$Reraise=1">
				<equip>
					<head>Twilight Helm</head>
					<body>Twilight Mail</body>
				</equip>
			</if>
			<if advanced="$Charm=1">
				<equip set="Charm" />
			</if>
			<if advanced="$Death=1">
				<equip set="Death" />
			</if>					
			<if BuffActive="2">
				<equip>
					<neck>Vim Torque</neck>
				</equip>
			</if>
			<if BuffActive="15">
				<equip>
					<waist>Gishdubar Sash</waist>
					<lring>Saida Ring</lring>
					<rring>Purity Ring</rring>
				</equip> 
			</if>	
			<if advanced="$CP=1">
				<equip lock="true">
					<back>Mecisto. Mantle</back>                
				</equip>
			</if>				
		</elseif>



		<elseif advanced="$TH=1">
			<equip>			
				<waist>Chaac Belt</waist>
			</equip>
		</elseif>

		<elseif advanced="$PDTSetActive=1">
			<equip set="PDT" />
			<if advanced="$Reraise=1">
				<equip>
					<head>Twilight Helm</head>
					<body>Twilight Mail</body>
				</equip>
			</if>
			<if advanced="$Charm=1">
				<equip set="Charm" />
			</if>
			<if advanced="$Death=1">
				<equip set="Death" />
			</if>				
				<if p_ismoving="True">
					<equip set="Movement" />
				</if>
			<if BuffActive="2">
				<equip>
					<neck>Vim Torque</neck>
				</equip>
			</if>
			<if BuffActive="15">
				<equip>
					<waist>Gishdubar Sash</waist>
					<lring>Saida Ring</lring>
					<rring>Purity Ring</rring>
				</equip> 
			</if>	
			<if advanced="$CP=1">
				<equip lock="true">
					<back>Mecisto. Mantle</back>                
				</equip>
			</if>				
		</elseif>


		<else>
			<if p_status="engaged">
			
				<!-- use of /Maj to do auto majesty while engaged -->
				<if advanced="$Maj=1">
					<if buffactive="621">
					</if>
					<else>
						<command>/ja Majesty me</command>
					</else>
				</if>
				
				<equip set="TP-$Set" />
				<if advanced="$Reraise=1">
					<equip>
						<head>Twilight Helm</head>
						<body>Twilight Mail</body>
					</equip>
				</if>
				<elseif advanced="$TH=1">
					<equip>
						<waist>Chaac Belt</waist>
					</equip>
				</elseif>
				<elseif advanced="$Reraise=1">
					<equip>
						<head>Twilight Helm</head>
						<body>Twilight Mail</body>
					</equip>
				</elseif>
				<if advanced="$Charm=1">
					<equip set="Charm" />
				</if>
				<if advanced="$Death=1">
					<equip set="Death" />
				</if>					
				<if BuffActive="2">
					<equip>
						<neck>Vim Torque</neck>
					</equip>
				</if>
				<if BuffActive="15">
					<equip>
						<waist>Gishdubar Sash</waist>
						<lring>Saida Ring</lring>
						<rring>Purity Ring</rring>
					</equip> 
				</if>	
				<if advanced="$CP=1">
					<equip lock="true">
					<back>Mecisto. Mantle</back>                
					</equip>
				</if>	
			</if>
			<else>

				<equip set="Idle" />
				<if e_area="Ru'Lude*|Nashmau|Tavnazian Safehold|Port*|Bastok*|Windurst*|*San d'Oria|*Jeuno|*Whitegate|*Adoulin|Selbina|Mhaura|Norg|Rabao|Mog*">
					<equip set="Town" />
				</if>
				<if advanced="$CP=1">
					<equip lock="true">
					<back>Mecisto. Mantle</back>                
					</equip>
				</if>	
				<if p_mpp="<50">
					<equip><waist>Fucho-no-Obi</waist></equip>
				</if>
				<if p_ismoving="True">
					<equip set="Movement" />
				</if>
				<if advanced="$Charm=1">
					<equip set="Charm" />
				</if>
				<if advanced="$Death=1">
					<equip set="Death" />
				</if>
				<if BuffActive="2">
					<equip>
						<neck>Vim Torque</neck>
					</equip>
				</if>
				<if BuffActive="15">
					<equip>
						<waist>Gishdubar Sash</waist>
						<lring>Saida Ring</lring>
						<rring>Purity Ring</rring>
					</equip> 
				</if>					
				<else>
					<if advanced="$Reraise=1">
						<equip>
							<head>Twilight Helm</head>
							<body>Twilight Mail</body>
						</equip>
					</if>
				</else>

			</else>
		</else>
		
		<!--Trial AUTO MODE -->
		<!-- Auto Defender/Majesty based on cmd /tank -->
		<if advanced="$Tank=0">
		</if>
		<else>
			<if buffactive="621">
			</if>
			<else>
				<command>/ja Majesty me</command>
			</else>
			<if p_subjob="WAR">
				<if buffactive="56">
					<command>/cancel 56</command>
				</if>
				<if buffactive="57">
				</if>
				<else>
					<command>/ja defender me</command>
				</else>
			</if>
		</else>
		<if advanced="$Auto=1">
			<if p_status="engaged">
				
				<if advanced="$Samba=2">
					<!--Attempting auto Drain Samba -->
					<if buffactive="368">
						<!-- Place holder for now -->
					</if>
					<else>
						<if p_tp=">250">
							<command>/Dsamba</command>
						</if>
					</else>
				</if>
				<elseif advanced="$Samba=1">
					<!--Attempting auto Haste Samba -->
					<if buffactive="370">
						<!-- Place holder for now -->
					</if>
					<else>
						<if p_tp=">450">
							<command>//Hastesamba</command>
						</if>
					</else>
				</elseif>
				
				
				<!-- Auto WS, Aedge at 1000TP, all else at 1750 for now -->
				
				<if advanced="$WS!=0">
					<if p_tp=">$TP">
						<command>/ws "$WSname"</command>
					</if>
				</if>
			</if>
		</if>
	</idlegear>

	<!-- That infamous theoretical ranged set is controlled here. -->
	<preranged>
		<equip set="Ranged" />
	</preranged>

	<!-- Various precast gear is controlled here. -->
	<premagic>	
		<equip set="FastCast" />
		<if ad_name="Cure*">
			<equip>
				<ear1>Mendi. Earring</ear1>
			</equip>
		</if>
		<elseif ad_name="Phalanx*">
			<equip>
				<waist>Siegel Sash</waist>
			</equip>
		</elseif>
	</premagic>

	<!-- Various swaps for midcast on spells are controlled here. -->
	<midmagic>
		<if advanced="$SIR=1" >
			<equip set="SIR" lock="true" />
		</if> 
		<if ad_name="Monomi*">
			<command>/cancel 71</command>
		</if>
		<elseif ad_name="Cure*">
			<if p_hp=">3200">
				<equip set="CureHP" />
			</if>
			<else>
				<equip set="Cure" />
			</else>
		
		</elseif>
		<elseif ad_name="Phalanx*">
			<equip set="Phalanx" />
		</elseif>
		<elseif ad_name="Enlight*">
			<equip set="Enlight" />
		</elseif>
		<elseif ad_name="Reprisal">
			<equip set="Reprisal" lock="true" />
		</elseif>		
		<elseif ad_name="Flash">
			<equip set="Flash" />
		</elseif>
		<elseif ad_name="Holy*">
			<equip set="Holy" />
		</elseif>
		<elseif ad_name="Utsusemi: Ichi">
			<!-- Cancels shadows if... -->
			<if buffactive="66">
				<!-- One is shadow left -->
				<command>/cancel 66</command>
			</if>
			<elseif buffactive="444">
				<!-- Two shadows are left -->
				<command>/cancel 444</command>
			</elseif>
			<else>
				<!-- Three are shadows left -->
				<command>/cancel 445</command>
			</else>
		</elseif>
		<else>
			<equip set="Haste" />				
		</else>
	</midmagic>

	<!-- This forces Ashitcast to swap your gear for Job Abilities. -->
	<jobability>                           
		<if ad_name="Provoke">
			<equip set="ENMITY" />
		</if>
		<elseif ad_name="Sentinel">
			<equip set="Sentinel" />
		</elseif>
		<elseif ad_name="Chivalry">
			<equip set="Chivalry" />
		</elseif>
		<elseif ad_name="Rampart">
			<equip set="Rampart" />
		</elseif>
		<elseif ad_name="Shield Bash">
			<equip set="Shield Bash" />
		</elseif>
		<elseif ad_name="Invincible">
			<equip set="Invincible" />
		</elseif>
		<elseif ad_name="Holy Circle">
			<equip set="Holy Circle" />
		</elseif>
		<elseif ad_name="Fealty">
			<equip set="Fealty" />
		</elseif>
		<elseif ad_name="Divine Emblem">
			<equip set="Divine Emblem" />
		</elseif>
		<elseif ad_name="Cover">
			<equip set="Cover" />
		</elseif>


		<if ad_name="Spectral Jig">
			<if buffactive="71">
				<command>/cancel 71</command>
				<cancel />
				<command delay="2000">/ja "%ad_name" %at_id</command>
			</if>
		</if>
	</jobability>

	<!-- These force Ashitcast to swap for your various WS. You can remove the
	HP checks if you'd like. These check your current HP and will swap to a
	WS set with more HP so your HP doesn't drop too much.-->
	<weaponskill>
	<if p_tp=">999">
		<if ad_name="Savage Blade">
			<if p_hp=">3200">
				<equip set="Savage-$SetHP" />
			</if>
			<else>
				<equip set="Savage-$Set" />
			</else>

		</if>
		<elseif ad_name="Requiescat"> 
			<if p_hp=">3200">
				<equip set="Req-$SetHP" />
			</if>
			<else>
				<equip set="Req-$Set" />
			</else>		
		
		
			<equip set="Req-$Set" />
		</elseif>
		<elseif ad_name="Chant du Cygne">
					<if p_hp=">3200"> <!-- BE CAREFUL OF THIS NUMBER, NEEDS ADJUSTING, I SET IT HIGH FOR NOW -->
				<equip set="Chant-$SetHP" />
			</if>
			<else>
				<equip set="Chant-$Set" />
			</else>
			<if e_time=">17.01|<6.59"><equip><ear1>Lugra Earring +1</ear1></equip></if>
		</elseif>
		<elseif ad_name="Sanguine Blade">
					<if p_hp=">3200">
				<equip set="Sang-$SetHP" />
			</if>
			<else>
				<equip set="Sang-$Set" />
			</else>

		</elseif>	
		<elseif ad_name="Aeolian Edge">
			<equip set="Aeolian-$Set" />
		</elseif>	
		<elseif ad_name="Atonement"> 
			<equip set="Atone-$Set" />
		</elseif>		
		<else>
			<equip set="Multi-$Set" />
		</else>
        
	</if>
	</weaponskill>
</ashitacast>