	<!-- Don't fiddle with these unless you know how it works -->
	<!-- This is how I've set up the option to have various gear sets via input commands such as /th or /eva. They all work, however they may not be coded 'optimally' so if you can clean it up, feel free -->
	<variables>
		<var name="init">1</var>
		<var name="Set">TP</var>
		<var name="EvasionSetActive">0</var>
		<var name="MDTSetActive">0</var>
		<var name="PDTSetActive">0</var>
		<var name="TH">1</var>
		<var name="WS">0</var>
		<var name="TP">1750</var>
		<var name="WSname">None</var>
		<var name="Auto">0</var>
		<var name="Stance">0</var>
		<var name="Samba">0</var>
		<var name="Step">0</var>
		<var name="Skillup">0</var>
		<var name="Proc">0</var>
		<var name="Dyna">0</var>
	</variables>

	<inputcommands>
		<cmd input="/init">
			<setvar name="init" value="0" />
		</cmd>
		<cmd input="/killall">
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="TH" value="0" />
			<addtochat color="4">KILLED IT ALL, son!</addtochat>
		</cmd>
		<cmd input="/skillup">
			<if advanced="$Skillup=0">
				<setvar name="Auto" value="1" />
				<command>/exec skillup1.txt</command>
				<addtochat color="4">SKILL UP SET EQUIPED</addtochat>
			</if>
			<elseif advanced="$Skillup=1">
				<setvar name="Skillup" value="0" />
				<command>/exec skillup2.txt</command>
				<addtochat color="4">SKILL UP SET UNEQUIPED</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/auto">
			<if advanced="$Auto=0">
				<setvar name="Auto" value="1" />
				<addtochat color="4">Auto Mode Engaged</addtochat>
			</if>
			<elseif advanced="$Auto=1">
				<setvar name="Auto" value="0" />
				<addtochat color="4">Auto Mode Disengaged</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/tp">
			<if advanced="$TP!=1000">
				<setvar name="TP" value="1000" />
				<addtochat color="4">AUTO TP SET TO 1000</addtochat>
			</if>
			<elseif advanced="$TP=1000">
				<setvar name="TP" value="1750" />
				<addtochat color="4">AUTO TP SET TO 1750</addtochat>				
			</elseif>		
		</cmd>
		<cmd input="/samba">
			<if advanced="$Samba=0">
				<setvar name="Samba" value="1" />
				<addtochat color="4">Haste Samba</addtochat>
			</if>
			<elseif advanced="$Samba=1">
				<setvar name="Samba" value="2" />
				<addtochat color="4">Drain Samba</addtochat>				
			</elseif>
			<elseif advanced="$Samba=2">
				<setvar name="Samba" value="0" />
				<addtochat color="4">NO SAMBA</addtochat>				
			</elseif>				
		</cmd>
		<cmd input="/step">
			<if advanced="$Step=0">
				<setvar name="Step" value="1" />
				<command>/ja "Quickstep"</command>
				<addtochat color="4">QUICK STEP</addtochat>
			</if>
			<elseif advanced="$Step=1">
				<setvar name="Step" value="0" />
				<command>/ja "Box Step"</command>
				<addtochat color="4">BOX STEP</addtochat>				
			</elseif>	
		</cmd>
		<cmd input="/stance">
			<if advanced="$Stance=0">
				<setvar name="Stance" value="1" />
				<addtochat color="4">SATA mode is set to SA</addtochat>
			</if>
			<elseif advanced="$Stance=1">
				<setvar name="Stance" value="2" />
				<addtochat color="4">SATA mode is set to TA</addtochat>				
			</elseif>
			<elseif advanced="$Stance=2">
				<setvar name="Stance" value="3" />
				<addtochat color="4">SATA mode is set to SATA</addtochat>				
			</elseif>
			<elseif advanced="$Stance=3">
				<setvar name="Stance" value="0" />
				<addtochat color="4">SATA mode is set to OFF</addtochat>				
			</elseif>			
		</cmd>
		<cmd input="/wskill">
			<if advanced="$WS=0">
				<setvar name="WS" value="1" />
				<setvar name="TP" value="1750" />
				<setvar name="WSname" value="Evisceration" />
				<addtochat color="4">Auto $WSname Enabled!</addtochat>
			</if>
			<elseif advanced="$WS=1">
				<setvar name="WS" value="2" />
				<setvar name="TP" value="1750" />
				<setvar name="WSname" value="Rudra's Storm" />
				<addtochat color="4">Auto $WSname Enabled!</addtochat>
			</elseif>
			<elseif advanced="$WS=2">
				<setvar name="WS" value="0" />
				<setvar name="TP" value="1750" />
				<setvar name="WSname" value="None" />
				<addtochat color="4">Auto WS Disabled!</addtochat>
			</elseif>
		</cmd>
		
		<cmd input="/proc">
			<if advanced="$Proc=0">
				<setvar name="Proc" value="1" />
				<command>/macro set 4</command>
				<command>/exec thfproc1.txt</command>
				<addtochat color="4">PROC Mode Engaged</addtochat>
			</if>
			<elseif advanced="$Proc=1">
				<setvar name="Proc" value="0" />
				<command>/macro set 3</command>
				<command>/exec thfproc2.txt</command>
				<addtochat color="4">PROC Mode Disengaged</addtochat>				
			</elseif>		
		</cmd>
		
		<cmd input="/dyna">
			<if advanced="$Dyna=0">
				<setvar name="Dyna" value="1" />
				<command>/macro set 1</command>
				<addtochat color="4">DYNA Mode Engaged</addtochat>
			</if>
			<elseif advanced="$Dyna=1">
				<setvar name="Dyna" value="0" />
				<addtochat color="4">DYNA Mode Disengaged</addtochat>				
			</elseif>		
		</cmd>
		
		<cmd input="/acc">
			<if advanced="$Set=TP">
				<setvar name="Set" value="ACC" />
				<addtochat color="4">Accuacy Set Equiped</addtochat>
			</if>
			<elseif advanced="$Set=ACC">
				<setvar name="Set" value="HYBRID" />
				<addtochat color="4">HYBRID Set Equiped</addtochat>
			</elseif>
			<elseif advanced="$Set=HYBRID">
				<setvar name="Set" value="TP" />
				<addtochat color="4">TP Set Equiped</addtochat>
			</elseif>
		</cmd>

		<!-- Treasure Hunter Toggle (If you want to replace the /th into
                                                 something more user friendly, you can alter it just below -->
		<cmd input="/th">
			<if advanced="$TH=0">
				<setvar name="TH" value="1" />
				<addtochat color="4">Treasure Hunter TP set enabled!</addtochat>
			</if>
			<else>
				<setvar name="TH" value="0" />
				<addtochat color="4">Treasure Hunter set TP disabled!</addtochat>
			</else>
		</cmd>

		<!-- Evasion set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/eva">
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<if advanced="$EvasionSetActive=0">
				<setvar name="EvasionSetActive" value="1" />
				<addtochat color="4">Evasion set enabled!</addtochat>
			</if>
			<else>
				<setvar name="EvasionSetActive" value="0" />
				<addtochat color="4">Evasion set disabled.</addtochat>
			</else>
		</cmd>
		<!-- MDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/mdt">
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />

			<if advanced="$MDTSetActive=0">
				<setvar name="MDTSetActive" value="1" />
				<addtochat color="4">MDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="MDTSetActive" value="0" />
				<addtochat color="4">MDT set disabled.</addtochat>
			</else>
		</cmd>
		<!-- PDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/pdt">
