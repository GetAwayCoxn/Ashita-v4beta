	<commands>
		<command input="/status">
			<setvar name="Status" value="0" />
		</command>
		<command input="/dt">
			<if advanced="$u_DT=Off">
				<setvar name="DT" value="On" />
			</if>
			<else>
				<setvar name="DT" value="Off" />
			</else>
			<setvar name="Status" value="0" />
		</command>
		<command input="/cape">
			<if advanced="$u_Cape=false">
				<setvar name="Cape" value="true" />
				<command>/exec capeon.txt</command>
				<echo mode="4">CP CAPE ON</echo>
			</if>
			<else>
				<setvar name="Cape" value="false" />
				<command>/exec capeoff.txt</command>
				<echo mode="4">CP CAPE OFF</echo>
			</else>
		</command>
		<command input="/af">
			<if advanced="$u_Body=Off">
				<setvar name="Body" value="On" />
			</if>
			<else>
				<setvar name="Body" value="Off" />
			</else>
			<setvar name="Status" value="0" />
		</command>
		<command input="/staff">
			<if advanced="$u_Weapon=Club">
				<setvar name="Weapon" value="Staff" />
			</if>
			<else>
				<setvar name="Weapon" value="Club" />
			</else>
			<setvar name="Status" value="0" />
		</command>
		<command input="/burst">
			<if advanced="$u_Set=Nuke">
				<setvar name="Set" value="Burst" />
			</if>
			<else>
				<setvar name="Set" value="Nuke" />
			</else>
			<setvar name="Status" value="0" />
		</command>
		<command input="/wcycle">
			<if advanced="$u_Element=0">
				<setvar name="Element" value="1" />
				<echo mode="4">Weather/Nukes set to BLIZZARD</echo>
				<setvar name="Dot" value="Cryohelix" />
				<setvar name="Weather" value="Hailstorm" />
				<setvar name="Nuke" value="Blizzard" />
			</if>
			<elseif advanced="$u_Element=1">
				<setvar name="Element" value="2" />
				<echo mode="4">Weather/Nukes set to FIRE</echo>
				<setvar name="Dot" value="Pyrohelix" />
				<setvar name="Weather" value="Firestorm" />
				<setvar name="Nuke" value="Fire" />
			</elseif>
			<elseif advanced="$u_Element=2">
				<setvar name="Element" value="3" />
				<echo mode="4">Weather/Nukes set to AERO</echo>
				<setvar name="Dot" value="Anemohelix" />
				<setvar name="Weather" value="Windstorm" />
				<setvar name="Nuke" value="Aero" />
			</elseif>
			<elseif advanced="$u_Element=3">
				<setvar name="Element" value="4" />
				<echo mode="4">Weather/Nukes set to STONE</echo>
				<setvar name="Dot" value="Geohelix" />
				<setvar name="Weather" value="Sandstorm" />
				<setvar name="Nuke" value="Stone" />
			</elseif>
			<elseif advanced="$u_Element=4">
				<setvar name="Element" value="5" />
				<echo mode="4">Weather/Nukes set to WATER</echo>
				<setvar name="Dot" value="Hydrohelix" />
				<setvar name="Weather" value="Rainstorm" />
				<setvar name="Nuke" value="Water" />
			</elseif>
			<elseif advanced="$u_Element=5">
				<setvar name="Element" value="0" />
				<echo mode="4">Weather/Nukes set to THUNDER</echo>
				<setvar name="Dot" value="Ionohelix" />
				<setvar name="Weather" value="Thunderstorm" />
				<setvar name="Nuke" value="Thunder" />
			</elseif>
		</command>
		<command input="/helix">
			<command>/ma "$u_Dot" </command>
		</command>
		<command input="/weather">
			<command>/ma "$u_Weather" me</command>
		</command>
		<command input="/nuke1">
			<command>/ma "$u_Nuke" </command>
		</command>
		<command input="/nuke2">
			<command>/ma "$u_Nuke II" </command>
		</command>
		<command input="/nuke3">
			<command>/ma "$u_Nuke III" </command>
		</command>
		<command input="/nuke4">
			<command>/ma "$u_Nuke IV" </command>
		</command>
		<command input="/nuke5">
			<command>/ma "$u_Nuke V" </command>
		</command>
		<command input="/nuke6">
			<command>/ma "$u_Nuke VI" </command>
		</command>
	</commands>

	<load>
		<setvar name="Status" value="0" />
		<setvar name="DT" value="Off" />
		<setvar name="Ring" value="None" />
		<setvar name="Set" value="Nuke" />
		<setvar name="Weapon" value="Club" />
		<setvar name="Element" value="0" />
		<setvar name="Dot" value="Ionohelix" />
		<setvar name="Weather" value="Thunderstorm" />
		<setvar name="Nuke" value="Thunder" />
		<setvar name="Cape" value="false" />
		<setvar name="Body" value="Off" />
		<command>/macro book 1</command>
		<if p_subjob="SCH">
			<command delay="500">/macro set 6</command>
		</if>
		<else>
			<command delay="500">/macro set 5</command>
		</else>
	</load>

	<default>
		<if advanced="$u_Status=0">
			<setvar name="Status" value="1" />
			<echo mode="4">$p_mainjob/$p_subjob STATUS:   DT [$u_DT]   SET [$u_Set]   WEAPON [$u_Weapon]   CYCLE [$u_Nuke]   AF BODY [$u_Body]</echo>
		</if>
		<if advanced="$u_DT=On">
			<equip set="DT" />
		</if>
		<elseif p_status="engaged">
			<equip set="TP" />
		</elseif>
		<else>
			<equip set="Idle-$u_Weapon" />
			<if p_ismoving="True">
				<equip set="Movement" />
			</if>
		</else>
		<if e_area="Ru'Lude*|Nashmau|Tavnazian Safehold|Port*|Bastok*|Windurst*|*San d'Oria|*Jeuno|*Whitegate|*Adoulin|Selbina|Mhaura|Norg|Rabao|Mog*">
			<equip set="Town" />
		</if>
		<if eq_rring="Trizek*|Capacity*|Endorsement*|Facility*|Dim. Ring*|Echad*|Warp*">
			<setvar name="Ring" value="%eq_rring" />
			<equip>
				<ring2>$u_Ring</ring2>
			</equip>
		</if>
		<if b_15=">=1">
			<equip>
				<ring1>Blenmot's Ring</ring1>
				<ring2>Purity Ring</ring2>
			</equip>
		</if>
		<if b_437=">=1">
			<equip>
				<feet>Goetia Sabots +2</feet>
				<back>Taranus's Cape</back>
			</equip>
		</if>
	</default>

	<precast>
		<equip set="Precast" />
		<if ad_type="whitemagic">
			<if ad_skill="healingmagic">
				<if ad_name="Cure*|Cura*">
					<equip set="CurePre" />
				</if>
			</if>
			<elseif ad_skill="enhancingmagic">
				<if ad_name="Stoneskin">
					<equip set="SSPre" />
				</if>
				<else>
					<equip set="EnhPre" />
				</else>
			</elseif>
		</if>
		<elseif ad_type="blackmagic">
			<if ad_skill="elementalmagic">
				<equip set="ElePre" />
				<if ad_name="Impact">
					<equip set="Impact" lock="true" />
				</if>
			</if>
			<elseif ad_skill="darkmagic">
				<if ad_name="Death">
					<equip set="DeathPre" lock="true" />
				</if>
			</elseif>
			<elseif ad_skill="enhancingmagic">
				<equip set="EnhPre" />
			</elseif>
		</elseif>
	</precast>

	<midcast>
		<equip set="Haste" />
		<if ad_type="whitemagic">
			<if ad_skill="healingmagic">
				<if ad_name="Cure*|Curaga*">
					<equip set="Cure" />
					<if mode="OR" e_weatherelement="Light" e_dayelement="Light">
						<equip>
							<waist>Hachirin-no-Obi</waist>
						</equip>
					</if>
				</if>
				<elseif ad_name="*na">
					<if ad_name="Cursna">
						<equip set="Cursna" />
					</if>
				</elseif>
			</if>
			<elseif ad_skill="enhancingmagic">
				<equip set="Enhancing" />
				<if ad_name="Stoneskin">
					<equip set="Stoneskin" />
				</if>
			</elseif>
			<elseif ad_skill="enfeeblingmagic">
				<equip set="Enfeebling" />
			</elseif>
		</if>
		<elseif ad_type="blackmagic">
			<if ad_skill="elementalmagic">
				<equip set="$u_Set-$u_Weapon" />
				<if ad_name="Impact">
					<equip set="Impact" lock="true" />
				</if>
